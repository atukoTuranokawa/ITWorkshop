<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
         version="3.1">
    <display-name>ExampleApp</display-name>

    <!-- DeleteMoodServlet -->
    <servlet>
        <servlet-name>DeleteMoodServlet</servlet-name>
        <servlet-class>com.example.test.DeleteMoodServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteMoodServlet</servlet-name>
        <url-pattern>/DeleteMoodServlet</url-pattern>
    </servlet-mapping>

    <!-- EditMoodServlet -->
    <servlet>
        <servlet-name>EditMoodServlet</servlet-name>
        <servlet-class>com.example.test.EditMoodServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditMoodServlet</servlet-name>
        <url-pattern>/EditMoodServlet</url-pattern>
    </servlet-mapping>

    <!-- H2Test -->
    <servlet>
        <servlet-name>H2Test</servlet-name>
        <servlet-class>com.example.test.H2Test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>H2Test</servlet-name>
        <url-pattern>/H2Test</url-pattern>
    </servlet-mapping>

    <!-- MoodFormServlet -->
    <servlet>
        <servlet-name>MoodFormServlet</servlet-name>
        <servlet-class>com.example.test.MoodFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MoodFormServlet</servlet-name>
        <url-pattern>/MoodFormServlet</url-pattern>
    </servlet-mapping>

    <!-- MoodListServlet -->
    <servlet>
        <servlet-name>MoodListServlet</servlet-name>
        <servlet-class>com.example.test.MoodListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MoodListServlet</servlet-name>
        <url-pattern>/MoodListServlet</url-pattern>
    </servlet-mapping>

    <!-- LoginServlet -->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.test.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <!-- SettingsServlet -->
    <servlet>
        <servlet-name>SettingsServlet</servlet-name>
        <servlet-class>com.example.test.SettingsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SettingsServlet</servlet-name>
        <url-pattern>/SettingsServlet</url-pattern>
    </servlet-mapping>

    <!-- SupportMoodListServlet -->
    <servlet>
        <servlet-name>SupportMoodListServlet</servlet-name>
        <servlet-class>com.example.test.SupportMoodListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SupportMoodListServlet</servlet-name>
        <url-pattern>/SupportMoodListServlet</url-pattern>
    </servlet-mapping>

</web-app>
